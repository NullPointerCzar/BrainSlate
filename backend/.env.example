# Example .env for backend
# Copy this file to .env and fill in the values below before running the server

# Port the backend server should listen on
PORT=5000

# MongoDB connection URI (local or Atlas)
# Example Atlas URI: mongodb+srv://<user>:<password>@cluster0.abcd.mongodb.net/brainstack?retryWrites=true&w=majority
MONGO_URI=mongodb://localhost:27017/brainstack

# Upstash Redis REST vars (optional â€” used by Redis.fromEnv())
# If you use Upstash, set the URL and token provided by Upstash
UPSTASH_REDIS_REST_URL=
UPSTASH_REDIS_REST_TOKEN=
